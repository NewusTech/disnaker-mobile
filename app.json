{
  "expo": {
    "name": "Disnaker Mobile Tanggamus",
    "slug": "disnaker-mobile",
    "version": "1.0.0A",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "disnaker",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.newus.disnaker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9dd6ca83-ab56-4c0e-9336-3f037ecde81b"
      }
    }
  }
}
